
#----------------------------- Global Properties ----------------------------#

/outputDir:bin/$(Platform)
/intermediateDir:obj/$(Platform)
/platform:DesktopGL
/config:
/profile:Reach
/compress:False

#-------------------------------- References --------------------------------#


#---------------------------------- Content ---------------------------------#

#begin fonts/MyFont.spritefont
/importer:FontDescriptionImporter
/processor:FontDescriptionProcessor
/processorParam:PremultiplyAlpha=True
/processorParam:TextureFormat=Compressed
/build:fonts/MyFont.spritefont

#begin sounds/8-bit-background-music-for-arcade-game-come-on-mario-164702.mp3
/importer:Mp3Importer
/processor:SongProcessor
/processorParam:Quality=Best
/build:sounds/8-bit-background-music-for-arcade-game-come-on-mario-164702.mp3

#begin sounds/fast-collision-reverb-14611.mp3
/importer:Mp3Importer
/processor:SoundEffectProcessor
